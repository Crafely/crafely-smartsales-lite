{
    "name": "crafely/crafely-smartsales-lite",
    "description": "AI-powered smart sales plugin for WooCommerce: automate inventory, orders, and insights with seamless integration and scalability.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "CSMSL\\Includes\\": "includes/",
            "CSMSL\\": "src/"
        },
        "files": [
            "includes/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CSMSL\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "alaminit",
            "email": "thisismdalamin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "^3.0",
        "phpcsstandards/phpcsutils": "^1.0",
        "phpcsstandards/phpcsextra": "^1.0"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "vendor/bin/phpcs --standard=phpcs.xml --report=summary",
        "lint:fix": "vendor/bin/phpcbf --standard=phpcs.xml",
        "lint:full": "vendor/bin/phpcs --standard=phpcs.xml --report=full",
        "phpcs": "vendor/bin/phpcs --standard=phpcs.xml --report=summary",
        "phpcbf": "vendor/bin/phpcbf --standard=phpcs.xml"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
